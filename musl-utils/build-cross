package() {
  for i in \
  a2x asciidoc \
  gtkdocize \
  help2man \
  ldconfig \
  makeinfo \
  po4a \
  texi2dvi texi2pdf; do
    cp -fPp "$core"/musl/files/true "$cross"/usr/bin/$i
  done
}
